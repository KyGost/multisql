{
	"$schema": "https://dprint.dev/schemas/v0.json",
	"projectType": "openSource",
	"incremental": true,
	"useTabs": true,
	"lineWidth": 100,
	"rustfmt": {
		"imports_layout": "vertical",
		"version": "Two"
	},
	"includes": ["**/*"],
	"excludes": [
		"**/book/**",
		"**/target/**",
		"**/*-lock.json"
	],
	"plugins": [
		"https://plugins.dprint.dev/typescript-0.45.0.wasm",
		"https://plugins.dprint.dev/json-0.10.2.wasm",
		"https://plugins.dprint.dev/markdown-0.7.1.wasm",
		"https://plugins.dprint.dev/rustfmt-0.3.0.wasm",
		"https://plugins.dprint.dev/prettier-0.2.2.exe-plugin@63b06beba3acd51e6d23379c47db342a1a24f3d88f3c52a775e2db3426124582",
		"https://github.com/thomaseizinger/dprint-plugin-cargo-toml/releases/download/0.1.0/cargo-toml-0.1.0.wasm"
	]
}
